  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "build": "rm -rf dist && node esbuild/mainBuild.js",
    "build:main": "npm run build -- main",
